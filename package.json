	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.scss": [
			"npm run lint:css"
		],
		"*.{js,ts,tsx}": [
			"prettier --write",
			"npm run lint:js"
		],
		"*.php": [
			"php -d display_errors=1 -l",
			"composer run-script phpcs"
		]
	},
	"files": [
		"assets/**/*.{js,scss,php}",
		"build/**/*.{js,json,css}",
		"includes/**/*.php",
		"languages/**/*.json",
		"license.txt",
		"elje.php"
	]
